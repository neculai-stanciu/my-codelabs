  "devDependencies": {
    "chokidar": "^3.5.3",
    "http-server": "^14.1.1",
    "reveal-md": "^5.4.1"
  },
  "dependencies": {
    "@simple-org/codelab-generator": "^1.0.9",
    "rimraf": "^4.4.0"
  }
